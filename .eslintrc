        "lines-around-comment": "error"/*"off"*/,
        //"lines-around-directive": "error"/*"off"*/, /*deprecated*/
        "max-depth": "error"/*"off"*/,
        "max-len": ["off", 120],
        "max-lines": ["warn"],
        "max-nested-callbacks": ["warn", 5],
        "max-params": ["warn", 5],
        "max-statements": ["warn"],
        "max-statements-per-line": "error"/*"off"*/,
        "multiline-ternary": ["off", "always"],
        "new-cap": "error"/*"off"*/,
        "new-parens": "error"/*"off"*/,
        //"newline-after-var": "error"/*"off"*/, /*deprecated*/
