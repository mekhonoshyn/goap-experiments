        "no-whitespace-before-property": "error"/*"off"*/,
        "no-with": "error"/*"off"*/,
        "nonblock-statement-body-position": "error"/*"off"*/,
        "object-curly-newline": ["off"],
        "object-curly-spacing": "error"/*"off"*/,
        "object-property-newline": "off",
        "one-var": ["error", "never"],
        "one-var-declaration-per-line": ["error", "always"],
        "operator-assignment": ["error", "always"],
        "operator-linebreak": ["error", "before"],
        "padded-blocks": ["error", "never"],
        "padding-line-between-statements": ["error",
            {"blankLine": "always", "prev": "*", "next": "return"},
            {"blankLine": "always", "prev": ["const", "let", "var"], "next": "*"},
            {"blankLine": "any",    "prev": ["const", "let", "var"], "next": ["const", "let", "var"]},
            {"blankLine": "always", "prev": "directive", "next": "*"},
            {"blankLine": "any",    "prev": "directive", "next": "directive" },
            {"blankLine": "always", "prev": "*", "next": "export" }
        ],
        "prefer-promise-reject-errors": "error"/*"off"*/,
        //"prefer-reflect": "warn", /*deprecated*/
        "quote-props": ["error", "as-needed"],
        "quotes": ["error", "single"],
        "radix": "error"/*"off"*/,
        "require-await": "error"/*"off"*/,
        "require-jsdoc": ["off"],
        "semi": ["error", "always"],
        "semi-spacing": ["error"],
        "semi-style": "error"/*"off"*/,
        "sort-keys": ["off"],
        "sort-vars": "error"/*"off"*/,
        "space-before-blocks": "error"/*"off"*/,
        "space-before-function-paren": ["error", {"anonymous": "always", "named": "never"}],
        "space-in-parens": "error"/*"off"*/,
        "space-infix-ops": "error"/*"off"*/,
        "space-unary-ops": "error"/*"off"*/,
        "spaced-comment": "error"/*"off"*/,
        "strict": ["error", "function"],
        "switch-colon-spacing": "error"/*"off"*/,
        "template-tag-spacing": "error"/*"off"*/,
        "unicode-bom": "error"/*"off"*/,
        "use-isnan": ["error"],
        "valid-jsdoc": "error"/*"off"*/,
        "valid-typeof": ["error"],
        "vars-on-top": ["warn"],
        "wrap-iife": ["error", "inside"],
        "wrap-regex": "error"/*"off"*/,
        "yoda": "error"/*"off"*/,

        //ES6
        "arrow-body-style": ["warn", "as-needed"],
        "arrow-parens": ["warn", "always"],
        "arrow-spacing": ["warn"],
        "constructor-super": ["warn"],
        "generator-star-spacing": ["warn"],
        "no-class-assign": ["warn"],
        "no-confusing-arrow": ["warn"],
        "no-const-assign": ["warn"],
        "no-dupe-class-members": ["warn"],
        "no-duplicate-imports": ["warn", { "includeExports": true }],
        "no-new-symbol": ["warn"],
        "no-restricted-imports": ["warn"],
        "no-this-before-super": ["warn"],
        "no-useless-computed-key": ["warn"],
        "no-useless-constructor": ["warn"],
        "no-useless-rename": ["warn"],
        "no-var": ["warn"],
        "object-shorthand": ["warn"],
        "prefer-arrow-callback": ["warn"],
        "prefer-const": ["warn"],
        "prefer-destructuring": ["warn"],
        "prefer-numeric-literals": ["warn"],
        "prefer-rest-params": ["warn"],
        "prefer-spread": ["warn"],
        "prefer-template": ["warn"],
        "require-yield": ["warn"],
        "rest-spread-spacing": ["warn", "never"],
        "sort-imports": ["off"],
        "symbol-description": ["warn"],
        "template-curly-spacing": ["warn"],
        "yield-star-spacing": ["warn"]
    }
