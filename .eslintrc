        "comma-dangle": ["error", "never"],
        "comma-spacing": "error"/*"off"*/,
        "comma-style": "error"/*"off"*/,
        "complexity": ["warn", 4],
        "computed-property-spacing": "error"/*"off"*/,
        "consistent-return": ["warn"], /*false-positive in case of getter-setter*/
        "consistent-this": ["error", "vm"],
        "curly": ["error", "all"],
        "default-case": "error"/*"off"*/,
        "dot-location": ["error", "property"],
        "dot-notation": "error"/*"off"*/,
        "eol-last": "error"/*"off"*/,
        "eqeqeq": "error"/*"off"*/,
        "for-direction": "error"/*"off"*/,
        "func-call-spacing": ["error"],
        "func-name-matching": "error"/*"off"*/,
        "func-names": ["error", "never"],
        "func-style": ["error", "declaration"],
        "global-require": "error"/*"off"*/,
        "guard-for-in": "error"/*"off"*/,
        "handle-callback-err": "error"/*"off"*/,
        "id-blacklist": "error"/*"off"*/,
        "id-length": "error"/*"off"*/,
        "id-match": "error"/*"off"*/,
        "indent": ["warn", 4, {"SwitchCase": 1, "MemberExpression": 1, "FunctionDeclaration": {"parameters": 1}}],
        //"indent-legacy": ["warn", 4], /*deprecated*/
        "init-declarations": ["warn", "always"],
        "jsx-quotes": "error"/*"off"*/,
        "key-spacing": "error"/*"off"*/,
        "keyword-spacing": ["error"],
        "line-comment-position": "error"/*"off"*/,
