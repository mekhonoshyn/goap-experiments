        "no-cond-assign": ["error"],
        "no-console": ["error"],
        "no-constant-condition": ["error"],
        "no-continue": "error"/*"off"*/,
        "no-control-regex": ["error"],
        "no-debugger": ["error"],
        "no-delete-var": ["error"],
        "no-div-regex": "error"/*"off"*/,
        "no-dupe-args": ["error"],
        "no-dupe-keys": ["error"],
        "no-duplicate-case": ["error"],
        "no-else-return": "error"/*"off"*/,
        "no-empty": ["error"],
        "no-empty-character-class": ["error"],
        "no-empty-function": ["error", {"allow": ["functions"]}],
        "no-empty-pattern": ["error"],
        "no-eq-null": "error"/*"off"*/,
        "no-eval": "error"/*"off"*/,
        "no-ex-assign": ["error"],
        "no-extend-native": "error"/*"off"*/,
        "no-extra-bind": "error"/*"off"*/,
        "no-extra-boolean-cast": ["error"],
        "no-extra-label": "error"/*"off"*/,
        "no-extra-parens": ["warn"],
        "no-extra-semi": ["error"],
        "no-fallthrough": ["error"],
        "no-floating-decimal": "error"/*"off"*/,
        "no-func-assign": ["error"],
        "no-global-assign": ["error"],
        "no-implicit-coercion": "error"/*"off"*/,
        "no-implicit-globals": "error"/*"off"*/,
        "no-implied-eval": "error"/*"off"*/,
        "no-inline-comments": "error"/*"off"*/,
        "no-inner-declarations": ["error"],
        "no-invalid-regexp": ["error"],
        "no-invalid-this": "error"/*"off"*/,
        "no-irregular-whitespace": ["error"],
        "no-iterator": "error"/*"off"*/,
        "no-label-var": "error"/*"off"*/,
        "no-labels": "error"/*"off"*/,
        "no-lone-blocks": "error"/*"off"*/,
        "no-lonely-if": "error"/*"off"*/,
        "no-loop-func": "error"/*"off"*/,
        "no-magic-numbers": ["warn", {"ignore": [-1, 0, 1], "ignoreArrayIndexes": true, "detectObjects": true}],
        "no-mixed-operators": ["warn", {
            "groups": [
                ["&&", "||"]
            ]
        }],
        "no-mixed-requires": "error"/*"off"*/,
        "no-mixed-spaces-and-tabs": ["error"],
        "no-multi-assign": "error"/*"off"*/,
        "no-multi-spaces": "error"/*"off"*/,
        "no-multi-str": "error"/*"off"*/,
        "no-multiple-empty-lines": ["error", {"max": 1}],
